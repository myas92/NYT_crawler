generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model nyt {
  id                Int      @id @default(autoincrement())
  date              String?  @unique
  url               String?  @unique
  questions_answers Json?
  questions         Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}
model answers_error {
  id                Int      @id @default(autoincrement())
  date              String?  
  url               String? 
  error             String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}
