generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model nyt {
  id                Int      @id @default(autoincrement())
  date              String?  @unique
  url_mini_cross    String?  @unique
  url_maxi_cross    String?  @unique
  questions_answers Json?
  questions         Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}

model nyt_maxi {
  id                Int      @id @default(autoincrement())
  qa_id              String?
  date              String?  @unique
  url_maxi_cross    String?  @unique
  questions_answers Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}

model nyt_mini {
  id                Int      @id @default(autoincrement())
  qa_id              String?
  date              String?  @unique
  url_mini_cross    String?  @unique
  questions_answers Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}

model answers_error {
  id         Int      @id @default(autoincrement())
  date       String?
  url        String?
  error      String?
  is_deleted Boolean  @default(false)
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now())
}

model seven_little_words {
  id                Int      @id @default(autoincrement())
  qa_id              String?
  date              String?  @unique
  url    String?  @unique
  questions_answers Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}

model response_info {
  id                Int      @id @default(autoincrement())
  qa_id             String?
  date              String?
  category          String?
  response          String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}

model daily_theme_maxi {
  id                Int      @id @default(autoincrement())
  qa_id             String?
  date              String?  @unique
  url               String?  @unique
  questions_answers Json?
  status            String?
  is_deleted        Boolean  @default(false)
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
}